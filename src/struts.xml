<struts>
	<!-- 请求参数的编码方式 -->
	<constant name="struts.i18n.encoding" value="UTF-8" />

	<!-- 指定由spring负责action对象的创建 -->
	<constant name="struts.objectFactory" value="spring" /> 
	<!-- 每次HTTP请求系统都重新加载资源文件，有助于开发(开发时设置为true,发布到生产环境后设置为false) -->
	<constant name="struts.i18n.reload" value="true" />

	<!-- 指定被struts2处理的请求后缀类型。多个用逗号隔开 -->
	<!-- <constant name="struts.action.extension" value="action,do,htm,html"/> -->

	<!-- 当struts.xml改动后，是否重新加载。默认值为false(生产环境下使用),开发阶段最好打开 -->
	<constant name="struts.configuration.xml.reload" value="true" />

	<!-- 是否使用struts的开发模式。开发模式会有更多的调试信息。默认值为false(生产环境下使用),开发阶段最好打开 -->
	<constant name="struts.devMode" value="false" />

	<!-- 设置浏览器是否缓存静态内容。默认值为true(生产环境下使用),开发阶段最好关闭 -->
	<constant name="struts.serve.static.browserCache" value="false" />

	<!-- 是否开启动态方法调用 -->
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />

	<!-- 配置文件上传最大值 -->
	<constant name="struts.multipart.maxSize" value="5000000000" />

	<constant name="struts.locale" value="zh_CN" />

	<constant name="struts.url.includeParams" value="none" />
	
	<!-- 配置不过滤webSocket请求 -->
	<constant name = "struts.action.excludePattern" value="/middleDateImportSocket,/summaryDateImportSocket,/batchTaskDetailImportSocket,/loadExcelSocket"></constant>
	
	
